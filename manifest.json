{
    "name": "JAVSAB",
    "version": "1.0",
    "description": "Just A Very Simple Ad Blocker",
    "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"],
    "background": {
        "scripts": ["blacklist.js", "listener.js"]
    },
    "browser_action": {
        "default_popup": "ui.html"
    },
    "manifest_version": 2
}
